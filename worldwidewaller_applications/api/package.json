{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "_controllers/*",
      "_models/*",
      "_services/*",
      "subsribers/*",
      "types/*",
      "./server.js"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "clean": "rimraf dist-server",
    "clean:all": "rimraf dist-server node_modules package-lock.json",
    "watch:dev": "nodemon",
    "build:dev": "rimraf dist && webpack --env.env=dev --env.app_type=api",
    "build:prod": "rimraf dist && webpack --env.env=prod --env.app_type=api",
    "server": "node ./dist/server.js",
    "dev": "npm-run-all clean build:dev server",
    "prod": "npm-run-all clean build:prod"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "express": "~4.18.2",
    "express-pino-logger": "^5.0.0",
    "file-loader": "^6.1.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "pino-colada": "^2.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
